    "@types/animejs": "^3.1.4",
    "@types/three": "^0.135.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "animejs": "^3.2.1",
    "autoprefixer": "^10.4.7",
    "cssnano": "^5.1.7",
    "eslint": "^7.32.0",
    "eslint-plugin-svelte3": "^3.4.1",
    "firebase": "^9.7.0",
    "graphql": "^15.8.0",
    "graphql-helix": "^1.12.0",
    "postcss": "^8.3.9",
    "postcss-load-config": "^3.1.0",
